BEGIN;

SELECT CASE WHEN NOT min_version('3.4.0') THEN plan(1) ELSE plan(5) END;

CREATE OR REPLACE FUNCTION types_check()
RETURNS SETOF TEXT AS
$BODY$
BEGIN

  IF NOT min_version('3.4.0') THEN
    RETURN QUERY
    SELECT skip(1, 'Function is new on 3.4.0');
    RETURN;
  END IF;

  RETURN QUERY SELECT has_function('pgr_cuthillmckeeordering');
  RETURN QUERY SELECT has_function('pgr_cuthillmckeeordering', ARRAY['text']);
  RETURN QUERY SELECT function_returns('pgr_cuthillmckeeordering', ARRAY['text'],  'setof record');

  RETURN QUERY
  SELECT function_args_eq('pgr_cuthillmckeeordering',
    $$SELECT  '{"","seq","node"}'::TEXT[] $$
  );

  RETURN QUERY
  SELECT function_types_eq('pgr_cuthillmckeeordering',
    $$VALUES
    ('{text,int8,int8}'::TEXT[])
    $$
  );
END;
$BODY$
LANGUAGE plpgsql;

SELECT types_check();

SELECT * FROM finish();
ROLLBACK;
