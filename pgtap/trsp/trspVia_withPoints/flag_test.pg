BEGIN;

UPDATE edges SET cost = sign(cost), reverse_cost = sign(reverse_cost);
SELECT CASE WHEN min_version('4.0.0') THEN plan(22) ELSE plan(1) END;

PREPARE edges_sql AS
SELECT id, source, target, cost, reverse_cost FROM edges ORDER BY id;

PREPARE restrictions_sql AS
SELECT path, cost FROM restrictions;

PREPARE points_sql AS
SELECT pid, edge_id, side, fraction FROM pointsOfInterest;

SELECT test_drivingSide('pgr_trspVia_withPoints');


SELECT finish();
ROLLBACK;
