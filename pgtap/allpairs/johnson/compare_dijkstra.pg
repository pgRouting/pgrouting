BEGIN;

UPDATE edges SET cost = sign(cost) + 0.001 * id * id, reverse_cost = sign(reverse_cost) + 0.001 * id * id;
SELECT plan(4);

SELECT allPairs_compare_dijkstra('pgr_johnson', true);
SELECT allPairs_compare_dijkstra('pgr_johnson', false);

SELECT finish();
ROLLBACK;
